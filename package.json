{
  "name": "project-front",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "serve": "serve -s dist",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dsfr-chart:build": "npm run build --prefix dsfr-chart && rm -rf src/components/DSFRChart && cp -r dsfr-chart/dist/* src/components/DSFRChart/",
    "pre-commit": "npm run dsfr-chart:build",
    "preview": "vite preview",
    "prestart": "react-dsfr update-icons; npm run dsfr-chart:build",
    "predev": "react-dsfr update-icons; npm run dsfr-chart:build",
    "prebuild": "react-dsfr update-icons"
  },
  "dependencies": {
