    "@mui/system": "^7.2.0",
    "@mui/x-charts": "^8.20.0",
    "@mui/x-data-grid": "^8.20.0",
    "@sentry/react": "^10.27.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
